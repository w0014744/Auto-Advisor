@model IEnumerable<CMPSAdvisingDB.Student>

@{
    Layout = "";
}

@Styles.Render("~/Content/css")

<table class="table">
    <tr>
        <th>
            First Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            W Nmber
        </th>
        <th>
            Hours
        </th>
        <th>
            GPA
        </th>
        <th>
            Concentration
        </th>
        <th>
            Adviser
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HoursCompleted)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GPA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Concentration.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Professor.LastName)
            </td>
           
        </tr>
    }

</table>